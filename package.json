{
  "name": "fastify-inertia-react",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "vite build --outDir build/client",
    "build:ssr": "vite build --outDir build/ssr --ssr src/ssr.jsx",
    "dev": "nodemon server.js",
    "lint": "eslint ./src",
    "start": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/session": "11.1.0",
    "@fastify/static": "8.2.0",
    "@inertiajs/react": "2.2.6",
    "express-session": "1.18.2",
    "fastify": "5.6.1",
    "fastify-inertiajs": "1.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "5.0.4",
    "cross-env": "10.1.0",
    "eslint": "9.37.0",
    "eslint-plugin-react-hooks": "6.1.1",
    "eslint-plugin-react-refresh": "0.4.23",
    "globals": "16.4.0",
    "nodemon": "3.1.10",
    "vite": "7.1.9"
  }
}
